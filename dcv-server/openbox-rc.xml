<?xml version="1.0" encoding="UTF-8"?>
<openbox_config xmlns="http://openbox.org/3.4/rc">
<theme>
  <name>Numix</name>
  <titleLayout>NLIMC</titleLayout>
  <keepBorder>yes</keepBorder>
  <animateIconify>yes</animateIconify>
  <font place="ActiveWindow">
    <name>Noto Sans</name>
    <size>10</size>
    <weight>bold</weight>
    <slant>normal</slant>
  </font>
  <font place="InactiveWindow">
    <name>Noto Sans</name>
    <size>10</size>
    <weight>normal</weight>
    <slant>normal</slant>
  </font>
  <font place="MenuHeader">
    <name>Noto Sans</name>
    <size>10</size>
    <weight>bold</weight>
    <slant>normal</slant>
  </font>
  <font place="MenuItem">
    <name>Noto Sans</name>
    <size>10</size>
    <weight>normal</weight>
    <slant>normal</slant>
  </font>
</theme>
</openbox_config>
